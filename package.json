{
  "name": "ligastavok",
  "private": true,
  "version": "0.0.0",
  "main": "server/index.js",
  "scripts": {
    "start": "DEBUG=app:*info,app:*warn,app:*error,app:*debug NODE_ENV=production CONFIG=production node `pwd`/server/index.js",
    "build": "better-npm-run build",
    "webpack": "webpack",
    "server": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p",
      "env": {
        "CONFIG": "production",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node server/index.js",
      "env": {
        "CONFIG": "development",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack-dev-server.js",
      "env": {
        "CONFIG": "development",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "dependencies": {
    "babel": "6.3.26",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "babel-register": "6.4.3",
    "better-npm-run": "0.0.5",
    "body-parser": "1.14.2",
    "compression": "1.6.1",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "express": "4.13.4",
    "history": "1.17.0",
    "pretty-error": "2.0.0",
    "query-string": "3.0.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "1.0.3",
    "redux": "3.0.6",
    "redux-router": "1.0.0-beta7",
    "redux-thunk": "1.0.3",
    "serialize-javascript": "1.1.2",
    "serve-favicon": "2.3.0",
    "webpack": "1.12.12",
    "webpack-isomorphic-tools": "2.2.26"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-preset-react-hmre": "1.0.1",
    "concurrently": "1.0.0",
    "css-loader": "0.23.1",
    "deep-merge": "1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "image-webpack-loader": "1.6.3",
    "piping": "0.3.0",
    "postcss-color-alpha": "1.0.3",
    "postcss-flexbugs-fixes": "2.0.0",
    "postcss-loader": "0.8.0",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "1.2.0",
    "postcss-vertical-rhythm": "1.1.5",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-devtools": "3.0.1",
    "redux-devtools-dock-monitor": "1.0.1",
    "redux-devtools-log-monitor": "1.0.2",
    "redux-logger": "2.4.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.6.4"
  }
}
